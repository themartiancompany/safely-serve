{
  "name":
    "safely-serve",
  "version":
    "0.0.7",
  "description":
    "Serve extension which auto-generates self-signed OpenSSL certificates' at runtime",
  "funding": {
    "type":
      "patreon",
    "url":
      "https://patreon.com/tallero"
  },
  "man":
    "./man/safely-serve.1",
  "files": [
    "AUTHORS.rst",
    "COPYING",
    "README.md",
    "eslint.config.mjs",
    "package.json",
    "safely-serve.js",
    "safely-serve",
    "fs-worker.js",
    "fs-worker.webpack.config.cjs",
    "index.html",
    "libsafely-serve",
    "libsafely-serve.webpack.config.cjs",
    "man",
    "serve.json",
    "webpack.config.cjs"
  ],
  "keywords": [
    "cert-tools",
    "ur",
    "themartiancompany",
    "dogeos"
  ],
  "homepage":
    "https://github.com/themartiancompany/safely-serve",
  "bugs": {
    "url":
      "https://github.com/themartiancompany/safely-serve/issues"
  },
  "repository": {
    "type":
      "git",
    "url":
      "git+https://github.com/themartiancompany/safely-serve.git"
  },
  "license":
    "AGPL-3.0-or-later",
  "author": {
    "name":
      "Pellegrino Prevete",
    "email":
      "pellegrinoprevete@gmail.com",
    "url":
      "https://github.com/tallero"
  },
  "devDependencies": {
    "crash-js-webpack":
      "^0.0.3",
    "eslint":
      "^9.32.0",
    "webpack":
      "^5.102.1",
    "webpack-cli":
      "^6.0.1"
  },
  "dependencies": {
    "crash-js":
      "^0.1.67",
    "cert-tools":
      "^0.0.7",
    "js-sha256":
      "^0.11.1",
    "os":
      "npm:os-browserify@^0.3.0",
    "serve":
      "^14.2.5"
  },
  "type":
    "commonjs",
  "main":
    "libsafely-serve",
  "bin":
    { "safely-serve":
        "safely-serve" },
  "scripts": {
    "build-fs-worker":
      "npx webpack --mode 'production' --config 'fs-worker.webpack.config.cjs' --stats-error-details",
    "build-lib":
      "npx webpack --mode 'production' --config 'libsafely-serve.webpack.config.cjs' --stats-error-details",
    "build-bin":
      "npx webpack --mode 'production' --config 'webpack.config.cjs' --stats-error-details",
    "build":
      "npm run lint && npm run build-fs-worker && npm run build-lib && npm run build-bin",
    "lint":
      "npx eslint .",
    "publish-pnm":
      "npm run build && npm publish --access public",
    "start":
      "npx safely-serve -d .",
    "test":
      "echo \"Error: no test specified\" && exit 1"
  }
}
